    "modules" : [
        {
            "name" : "gnome-text-editor",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://gitlab.gnome.org/chergert/gnome-text-editor.git",
                    "tag" : "41.beta1",
                    "commit" : "3508525d9285143d24c98ee27475e2d7809fbc59"
                }
            ]
        }
    ]
